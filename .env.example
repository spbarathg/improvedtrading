# Security-sensitive fields (replace with your values)
PRIVATE_KEY=your_solana_private_key_here

# Network endpoints
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
JUPITER_API_URL=https://quote-api.jup.ag/v4

# Trading parameters
TRADING_PAIRS=["SOL/USDC","RAY/USDC"]
MAX_POSITION_SIZE=1.0
SLIPPAGE_BPS=50

# Risk management
STOP_LOSS_PCT=5.0
TAKE_PROFIT_PCT=10.0
MAX_DAILY_LOSS_PCT=2.0
MAX_OPEN_TRADES=5

# Performance tuning
RPC_RATE_LIMIT=10
JUPITER_RATE_LIMIT=5
CACHE_SIZE=1000
CACHE_TTL=3600

# Data pipeline
DATA_STORAGE_PATH=data/trading_bot.db

# System configuration
TRADING_ENABLED=false
LOG_LEVEL=INFO
LOG_FILE=logs/trading_bot.log

# AI Model Configuration
MODEL__MAX_PREDICTIONS_PER_SECOND=10
MODEL__MAX_CONCURRENT_PREDICTIONS=5
MODEL__MAX_BURST_SIZE=20

# Resource limits
MODEL__MAX_MEMORY_USAGE_MB=2000
MODEL__MAX_CPU_USAGE_PCT=70.0

# Performance monitoring
MODEL__METRICS_ENABLED=true
MODEL__METRICS_LOG_INTERVAL=300
MODEL__METRICS_RETENTION_DAYS=30

# Circuit breaker settings
MODEL__CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
MODEL__CIRCUIT_BREAKER_RECOVERY_TIMEOUT=60
MODEL__CIRCUIT_BREAKER_HALF_OPEN_TIMEOUT=30 